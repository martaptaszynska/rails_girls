<div>
  <%= form_for @photo do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :description %>:
  <%= f.text_field :description %><br />
  
   <%= f.label :photo_url %>:
  <%= f.text_field :photo_url %><br />
  
  <%= f.label :date %>:
  <%= f.text_field :date %><br />

  <%= f.submit %>
   <% end %>
  </div>